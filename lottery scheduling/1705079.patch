diff --git a/prcst.c b/prcst.c
new file mode 100644
index 0000000..d2b9969
--- /dev/null
+++ b/prcst.c
@@ -0,0 +1,19 @@
+#include "types.h"
+#include "stat.h"
+#include "user.h"
+#include "fcntl.h"
+#include "random.c"
+
+int main(int argc, char *argv[])
+{
+    struct pstat ps;
+    int len = getpinfo(&ps);
+    int i;
+    settickets(random_at_most(20));
+    for (i = 0; i < len; i++)
+    {
+        if (ps.pid[i] != 0)
+            printf(1, "process %d has ticks = %d ,tickets = %d , inuse = %d\n", ps.pid[i], ps.ticks[i], ps.tickets[i], ps.inuse[i]);
+    }
+    exit();
+}
